package package1;

import java.util.*;

public class Menu {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		Insurance insurance = new Insurance();
		
		int age;
		int not;
		long voc;
		
		System.out.print("DRIVER'S AGE? ");
		age = input.nextInt();
		
		System.out.print("NUMBER OF TICKETS? ");
		not = input.nextInt();
		
		System.out.print("VALUR OF CAR? ");
		voc = input.nextLong();
		
		System.out.printf("Premium: %f" , 
		insurance.autoInsurance(age, not, voc));
		
		input.close();
	}

}
