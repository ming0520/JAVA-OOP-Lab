package package1;

import java.util.*;

public class isPrime {

	public static void main(String[] args) {
		
		Scanner input = new Scanner(System.in);
		int number = 1, prime;
		
		System.out.print("Enter number to check prime number: ");
		prime = input.nextInt();
		
		System.out.println("Prime number:");
		for(int i = 0; i < prime - 1; i++) {
			prime = 1
		}

	}

}
