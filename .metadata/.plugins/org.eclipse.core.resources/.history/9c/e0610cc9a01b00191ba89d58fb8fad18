package package1;

public class MMath {
	
	int number1, number2;
	
	public void setNumber(int num1, int num2) {
		
	}
	
	public boolean isDevisible() {
	}
	
	public void compare (int number1,int number2) {
		int swap;
		
		if(number1 != number2) {
			if (number1 < number2) {
				swap = number1;
				number1= number2;
				number2 = swap;
			}
			System.out.println(number1 +" is Greater than " + number2);
		}
		else {
			System.out.println(number1 + " is Equal to " + number2);
		}		
	}
}
