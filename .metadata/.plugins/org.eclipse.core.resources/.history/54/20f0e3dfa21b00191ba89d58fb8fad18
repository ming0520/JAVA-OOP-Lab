package package1;

public class MMath {
	
	int number1, number2;
	
	public void greater(int num1, int num2) {
		this.number1 = num1;
		this.number2 = num2;
		
		if(this.number1 != this.number2) {
				swap();
				System.out.println(this.number1 +" is Greater than " + this.number2);
		}
		else {
			System.out.println(this.number1 + "is Equal to " + this.number2);
		}
	}
	
	public void swap () {
		int swap;
		if(this.number1 > this.number2) {
			swap = this.number1;
			this.number1 = this.number2;
			this.number1 = swap;
		}
	}
	
	public void isDevisible() {
		int remainder = this.number1 % this.number2;
		
		switch(remainder) {
			case 0:
				System.out.println(this.number1 + " is Devisible " + this.number2);
				break;
			default:
				System.out.println(this.number1 + " is Not Devisible " + this.number2);
		}
	}
	
}
