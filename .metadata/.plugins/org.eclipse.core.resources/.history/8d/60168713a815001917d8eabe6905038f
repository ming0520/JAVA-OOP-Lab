package pack1;

public class Concert {
	
	int		   NUMBER_OF_CATEGORIES;
	int		[] PRICE_OF_TICKETS      = new 	int		[NUMBER_OF_CATEGORIES];
	String	[] NAME_OF_CATEGORIES    = new 	String	[NUMBER_OF_CATEGORIES];
	int		[] tickets_sold 		 = new 	int		[NUMBER_OF_CATEGORIES];
	int		   total_revenue;
	
	public void setNumberOfCategories(int noc) {
		this.NUMBER_OF_CATEGORIES = noc;
	}
	
	public void setNameOfCategories (String name[]) {
		this.NAME_OF_CATEGORIES = name[];
	}

}
